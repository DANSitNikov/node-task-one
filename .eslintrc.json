{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-restricted-syntax": 0,
        "func-names": 0,
        "no-underscore-dangle": 0
    }
}
